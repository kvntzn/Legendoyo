        <arguments>
          <argument>
            <kind>1</kind>
            <string>///control the menu
if(alarm[0] &lt;= 0){
    if(obj_input.key_down){
        if (menu_index &lt; array_length_1d(option)-1){
            menu_index++;
        }else{
            menu_index = 0;
        }
        alarm[0] = room_speed/6;
    }
    
    if(obj_input.key_up){
        if(menu_index &gt; 0){
            menu_index --;
        }else{
            
            menu_index = array_length_1d(option)-1;        
        }
        alarm[0] = room_speed/6;
    }
    
    if(obj_input.key_enter){
        switch (menu_index){
            case 0: 
                 scr_overwrite_game();
                break;
            
           case 1: 
                scr_check_load_game();
                game_started = 1;
              //   room_goto(rm_game);
                break;
            
            case 2:
                room_goto(rm_help);
                break;
            
           
               case 3:
                room_goto(rm_about);
                break; 
           
                
            default:
                break;       
        
        }
        obj_input.key_enter = false;
    }
    
}
</string>
